# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Messenger' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Messenger
  
  #Firebase
  pod 'Firebase/Firestore'
  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  
  #Facebook Login
  pod 'FBSDKLoginKit'
  
  #Google Login
  pod 'GoogleSignIn'

  #Dependantes
  pod 'MessageKit'
  pod 'JGProgressHUD'
  pod 'RealmSwift'
  pod 'SDWebImage'
  
end
